<!-- @format -->

<!-- Add this line at the top of your partial if you want to include the stylesheet from this partial -->
<link
	rel="stylesheet"
	href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.7.2/css/all.min.css"
	integrity="sha512-Evv84Mr4kqVGRNSgIGL/F/aIDqQb7xQ2vcrdIwxfjThSH8CSR7PBEakCr51Ck+w+/U6swU2Im1vVX0SVk9ABhg=="
	crossorigin="anonymous"
	referrerpolicy="no-referrer" />
<header class="glass">
	<div class="header-actions">
		<button class="glass menu-button" aria-label="Toggle Navigation">
			<i class="carved-glass fa-solid fa-bars" aria-label="Toggle Navigation"></i>
		</button>
		<a href="/login/signup" class="glass button-link" id="signupBtn">
			<span class="carved-glass">Sign Up</span>
		</a>
		<a href="/login" class="glass button-link" id="loginBtn">
			<span class="carved-glass">Login</span>
		</a>
	</div>
	<h1 class="embossed-glass"><%= currentPage %></h1>
</header>
